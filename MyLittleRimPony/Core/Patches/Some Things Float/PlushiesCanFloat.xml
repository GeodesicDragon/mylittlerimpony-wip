<Patch>

<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Some Things Float</li>
	</mods>
	<match Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[@Name="PonyPlushBase"]</xpath>
		<value>
			<li Class="SomeThingsFloat.FloatingThing_ModExtension">
				<floatingValue>0.5</floatingValue>
			</li>
		</value>
	</match>
</Operation>

</Patch>